{
  "name": "@nextcapital/copy-service-integration-tests",
  "scripts": {
    "build": "webpack",
    "lint": "eslint \"tests/*.js?(x)\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BLC/copy-service.git"
  },
  "author": "kreiserm@nextcapital.com",
  "bugs": {
    "url": "https://github.com/BLC/copy-service/issues"
  },
  "homepage": "https://github.com/BLC/copy-service#readme",
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^@nextcapital/copy-service$": "<rootDir>/node_modules/@nextcapital/copy-service/dist/copy-service.js",
      "^@nextcapital/plain-text-evaluator$": "<rootDir>/node_modules/@nextcapital/plain-text-evaluator/dist/plain-text-evaluator.js",
      "^@nextcapital/react-evaluator$": "<rootDir>/node_modules/@nextcapital/react-evaluator/dist/react-evaluator.js",
      "^React$": "<rootDir>/node_modules/react/dist/react.js"
    },
    "testMatch": [
      "<rootDir>/tests/*.test.{js,jsx}"
    ],
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  },
  "devDependencies": {
    "@nextcapital/copy-service": "*",
    "@nextcapital/eslint-config-js-linter": "1.0.0",
    "@nextcapital/plain-text-evaluator": "*",
    "@nextcapital/react-evaluator": "*",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^22.4.3",
    "jest-junit-reporter": "^1.1.0",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  }
}
