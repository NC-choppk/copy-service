{
  "name": "@nextcapital/plain-text-evaluator",
  "version": "1.0.2",
  "description": "An Evaluator that returns plain text without formatting or evaluated functions.",
  "main": "dist/plain-text-evaluator.min.js",
  "files": [
    "dist/plain-text-evaluator*"
  ],
  "scripts": {
    "autopublish": "autopublish",
    "build": "webpack",
    "docs": "jsdoc PlainTextEvaluator.js -c .jsdoc.json",
    "lint": "eslint PlainTextEvaluator.js PlainTextEvaluator.test.js",
    "prepare": "rm -rf dist && npm run build -- --silent",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BLC/copy-service.git"
  },
  "author": "piercej@nextcapital.com",
  "bugs": {
    "url": "https://github.com/BLC/copy-service/issues"
  },
  "homepage": "https://github.com/BLC/copy-service#readme",
  "publishConfig": {
    "registry": "https://npm.nextcapital.com"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "PlainTextEvaluator.js"
    ],
    "globals": {
      "DEV_MODE": true
    },
    "testMatch": [
      "<rootDir>/PlainTextEvaluator.test.js"
    ],
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  },
  "devDependencies": {
    "@nextcapital/copy-service": "1.1.0",
    "@nextcapital/eslint-config-js-linter": "1.0.0",
    "autopublish": "0.0.8",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.4.3",
    "jest-junit-reporter": "^1.1.0",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  },
  "peerDependencies": {
    "@nextcapital/copy-service": "1.0.4`",
    "lodash": "^4.17.10"
  }
}
